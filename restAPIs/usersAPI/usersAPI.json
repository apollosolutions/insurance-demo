{
    "openapi": "3.0.0",
    "info": {
      "title": "UsersAPI",
      "version": "1.0.0"
    },
    "paths": {
      "/user": {
        "get": {
          "responses": {
            "200": {
              "description": "",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/user"
                  }
                }
              }
            }
          }
        }
      },
      "/users": {
        "get": {
          "responses": {
            "200": {
              "description": "",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "array",
                    "items": {
                      "$ref": "#/components/schemas/user"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "components": {
      "schemas": {
        "user": {
          "type": "object",
          "required": [
            "id",
            "dateOfBirth"
          ],
          "properties": {
            "id": {
              "example": "3",
              "type": "string"
            },
            "firstName": {
              "example": "Jane",
              "type": "string"
            },
            "lastName": {
              "example": "Doe",
              "type": "string"
            },
            "emailAddress": {
              "example": "janedoe@emailcompany.org",
              "type": "string"
            },
            "phoneNumber": {
              "example": "4709998877",
              "type": "string"
            },
            "dateOfBirth": {
              "default": "2011-05-10",
              "example": "2025-02-28",
              "type": "string",
              "format": "date"
            }
          }
        }
      }
    }
  }
  
  
  
  
  